    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_left">

    <fragment
        android:id="@+id/navigation_left"
        android:name="dma.pwr.lab3.ui.middle.MiddleFragment"
